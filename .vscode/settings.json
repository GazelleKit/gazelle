{
  "[fsharp]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "Ionide.Ionide-fsharp"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/obj": true,
    "**/bin": true,
    "**/.idea": true,
    "*.sln.DotSettings.user": true,
    "**/*.fs.js": true
  },
  "liveServer.settings.port": 8080,
  "liveServer.settings.root": "/projects/calcpad/www",
  "liveServer.settings.file": "index.html"
}